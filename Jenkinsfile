pipeline {
    agent any 
   
    stages{
        stage("Compile"){
            steps{
                script{
                    echo "Compiling the code with masteringgit status"
                }
                
            }
            
        }
        
          stage("UnitTest"){
            steps{
                script{
                    echo "Testing the unit test cases,"
                }
                
            }
            
        }
        
          stage("Packing "){
            steps{
                script{
                    echo "Packaging the code"
                }
                
            }
            
        }
      
      
        
    }
    
}
    
